int sayi;
Console.Write("Bir sayı giriniz:");
sayi = Convert.ToInt32(Console.ReadLine());
if (sayi % 2 == 0)
{
     Console.WriteLine("Sayi çiftir.");
}
else
{
     Console.WriteLine("Sayi tekdir.");
}
Console.ReadKey();
